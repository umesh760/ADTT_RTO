<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical"
    tools:context="com.rto_driving_test.Authorization.ActShowRoute">

    <include layout="@layout/tool_bar_layout"></include>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/appbar"
            android:background="@color/bg_color">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/bg_color"
                android:orientation="vertical">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <RelativeLayout
                        android:id="@+id/rl_name"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:padding="4dp"
                        >

                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="fill_parent"
                            android:layout_height="30dp"
                            android:layout_toRightOf="@+id/rl_date"
                            android:gravity="center|left"
                            android:text="Rakesh Verma"
                            android:paddingLeft="10dp"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_id"
                            android:layout_width="fill_parent"
                            android:layout_height="30dp"
                            android:layout_toRightOf="@+id/rl_date"
                            android:layout_below="@+id/tv_name"
                            android:gravity="center|left"
                            android:text="ID:24020202023"
                            android:paddingLeft="10dp"
                            android:textSize="14dp" />
                        <RelativeLayout
                            android:id="@+id/rl_date"
                            android:layout_width="100dp"
                            android:layout_height="fill_parent">

                            <ImageView
                                android:id="@+id/applicant_img"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/pic" />
                        </RelativeLayout>

                    </RelativeLayout>

                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Test Type"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_ap_no"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Four Wheeler"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >
                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Track Path Traversed"
                            android:textSize="14dp"
                            />
                        <TextView
                            android:id="@+id/tv_nm"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="3 "
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Track Path Traverse"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_fath_nm"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Allotted Time(in Sec)"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_dob"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Start Time"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_dl_no"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="14:20:30"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="End Time"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_date"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="15:20:30"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Duration of Test(in Sec)"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_time"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="13"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="center|left"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        >

                        <TextView
                            android:layout_width="@dimen/resutl_txt_size"
                            android:layout_height="wrap_content"
                            android:text="Status"
                            android:textSize="14dp" />

                        <TextView
                            android:id="@+id/tv_vehicle_status"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Pass"
                            android:textSize="14dp" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/button_height"
                    android:layout_marginBottom="@dimen/center_info_left_righ_margin"
                    android:layout_marginLeft="@dimen/center_info_left_righ_margin"
                    android:layout_marginRight="@dimen/center_info_left_righ_margin"
                    android:layout_marginTop="30dp"
                    >
                    <Button
                        android:id="@+id/btn_submit_detail"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/login_rectangle"
                        android:text="@string/back_to_list"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_normal" />
                </RelativeLayout>

            </LinearLayout>
        </ScrollView>
        <include layout="@layout/error_layout"> </include>
    </RelativeLayout>

</LinearLayout>
